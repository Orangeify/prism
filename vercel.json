{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "public" }
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/wisp/(.*)",
      "dest": "/_empty_wisp",
      "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"]
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}
